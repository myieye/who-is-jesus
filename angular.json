{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "who-is-jesus": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/.htaccess"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [
              "src/scripts/web-init.js",
              "src/scripts/webfont-loader.js"
            ]
          },
          "configurations": {
            "production": {
              "baseHref": "/who-is-jesus/",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/app/content/de/verses.de.ts",
                  "with": "src/app/content/de/verses.de.ng√º.__private.v1.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "cordova": {
              "baseHref": "./",
              "outputPath": "www",
              "scripts": [
                "src/scripts/script-loader.js",
                "src/scripts/cordova-init.js"
              ],
              "styles": [
                "src/styles.scss",
                "src/cordova/fonts/fonts.css"
              ],
              "assets": [
                {
                  "glob": "bible-gateway-icon.ico",
                  "input": "src/assets",
                  "output": "./assets"
                },
                {
                  "glob": "**/*",
                  "input": "platforms/android/platform_www",
                  "output": "./cordova/android"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/app/content/language-configs.ts",
                  "with": "src/app/content/mobile-language-configs.__private.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "who-is-jesus:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "who-is-jesus:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "who-is-jesus:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "who-is-jesus:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "who-is-jesus:serve:production"
            }
          }
        },
        "deploy": {
          "builder": "angular-cli-ghpages:deploy",
          "options": {
            "branch": "master",
            "baseHref": "/who-is-jesus/"
          }
        }
      }
    }
  },
  "defaultProject": "who-is-jesus",
  "cli": {
    "analytics": "ce2a2b91-964a-489f-aac2-82922650629a"
  }
}