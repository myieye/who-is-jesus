{
  "name": "com.timhaasdyk.whoisjesus",
  "displayName": "Who is Jesus?",
  "description": "A streamlined tool for exploring the biblical greatness of Jesus in the gospels.",
  "author": "Tim Haasdyk",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev": "ng serve --host 0.0.0.0",
    "prod": "ng serve --prod=true --host 0.0.0.0",

    "prebuild-prod": "npm run build-online-content",
    "build-prod": "ng build --prod=true --aot",

    "prebuild-cordova": "npm run build-online-content",
    "build-cordova": "ng build --configuration=production,cordova --aot",

    "build-online-content": "tsc --build tsconfig.online-content.json"
  },
  "private": true,
  "optionalDependencies": {},
  "dependencies": {
    "@angular/animations": "~9.0.0-rc.10",
    "@angular/cdk": "~8.2.3",
